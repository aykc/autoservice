.mb-3
  = form.label :service_id, 'Услуга', class: "form-label"
  = form.select :service_id, Service.all.collect{|s| [s.name, s.id] }, {}, class: "form-select"
.mb-3
  = form.label :user_id, 'Исполнитель', class: "form-label"
  = form.select :user_id, User.all.collect{|s| [s.name, s.id] }, {}, class: "form-control form-control-md"
.mb-3
  = form.submit class: "btn btn-primary"

